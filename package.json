{
  "name": "@newhighsco/destiny-clan-warfare",
  "description": "Destiny Clan Warfare",
  "version": "5.8.16",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/destiny-clan-warfare.git"
  },
  "homepage": "https://destinyclanwarfare.com/",
  "bugs": {
    "url": "https://github.com/newhighsco/destiny-clan-warfare/issues"
  },
  "files": [],
  "scripts": {
    "prepare": "is-ci || husky install",
    "clean": "rm -rf {.next,coverage,public/{robots.txt,sitemap*.xml},storybook}",
    "start": "netlify dev",
    "start:next": "next dev --port 9001",
    "start:storybook": "storybook dev --port 9002 --docs",
    "build": "next build",
    "postbuild": "next-sitemap",
    "build:storybook": "storybook build --output-dir storybook --docs --quiet",
    "serve": "next start",
    "pretest": "yarn lint",
    "test": "jest",
    "snapshot": "chromatic --storybook-build-dir storybook --auto-accept-changes main --exit-once-uploaded",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "yarn lint-staged:js --cache .",
    "lint:css": "yarn lint-staged:css --cache '**/*.?(s)css'",
    "lint-staged:js": "eslint --ignore-path .gitignore",
    "lint-staged:css": "stylelint --ignore-path .gitignore",
    "format": "npm-run-all --parallel format:*",
    "format:js": "yarn lint:js --fix",
    "format:css": "yarn lint:css --fix",
    "pregenerate:ssl": "mkdir -p ssl",
    "generate:ssl": "openssl req -x509 -out ssl/localhost.crt -keyout ssl/localhost.key -newkey rsa:2048 -nodes -sha256 -subj '/CN=localhost' -extensions EXT -config certconf"
  },
  "dependencies": {
    "@newhighsco/chipset": "6.15.1",
    "@newhighsco/next-plugin-svgr": "3.0.109",
    "@newhighsco/press-start": "2.2.12",
    "autolinker": "4.0.0",
    "bungie-api-ts": "5.0.0",
    "dayjs": "1.11.9",
    "json-bigint": "1.0.0",
    "next": "13.4.19",
    "next-auth": "4.23.1",
    "next-compose-plugins": "2.2.1",
    "next-sitemap": "4.2.2",
    "param-case": "3.0.4",
    "postcss": "8.4.28",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-virtualized-auto-sizer": "1.0.20",
    "react-window": "1.8.9",
    "sass": "1.66.1",
    "sentence-case": "3.0.4"
  },
  "devDependencies": {
    "@babel/core": "7.22.10",
    "@commitlint/cli": "17.7.1",
    "@netlify/plugin-nextjs": "4.40.0",
    "@newhighsco/browserslist-config": "1.1.0",
    "@newhighsco/commitlint-config": "1.1.19",
    "@newhighsco/editor-config": "1.2.0",
    "@newhighsco/eslint-config": "4.0.57",
    "@newhighsco/postcss-config": "3.5.146",
    "@newhighsco/prettier-config": "2.1.14",
    "@newhighsco/release-config": "1.3.24",
    "@newhighsco/storybook-preset": "5.1.53",
    "@newhighsco/stylelint-config": "3.1.57",
    "@newhighsco/svgo-config": "1.0.1",
    "@storybook/manager-api": "7.3.2",
    "@storybook/react-webpack5": "7.3.2",
    "@storybook/theming": "7.3.2",
    "@testing-library/jest-dom": "6.0.1",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.3",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/react-window": "1.8.5",
    "alias-hq": "6.2.2",
    "babel-loader": "9.1.3",
    "chromatic": "6.21.0",
    "eslint": "8.47.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.6.2",
    "jest-environment-jsdom": "29.6.2",
    "lint-staged": "14.0.1",
    "netlify-cli": "16.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.2",
    "semantic-release": "21.0.9",
    "storybook": "7.3.2",
    "stylelint": "15.10.3",
    "typescript": "5.1.6",
    "webpack": "5.88.2"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config"
    ]
  },
  "lint-staged": {
    "*.{[j|t]s?(on|x),mdx}": "yarn lint-staged:js --fix",
    "*.?(s)css": "yarn lint-staged:css --fix"
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
