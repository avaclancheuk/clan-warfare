{
  "name": "destiny-clan-warfare",
  "description": "Destiny Clan Warfare",
  "version": "1.3.29",
  "author": "Avalanche UK <admins@avaclanche.uk>",
  "license": "ISC",
  "engines": {
    "node": "^8.9.0",
    "yarn": "^1.2.1"
  },
  "scripts": {
    "start": "npm-run-all --parallel start:modernizr start:gatsby",
    "start:modernizr": "npm run build:modernizr",
    "start:gatsby": "node --max-old-space-size=8192 ./node_modules/.bin/gatsby develop",
    "build": "npm-run-all build:modernizr build:patch build:gatsby",
    "build:modernizr": "modernizr -c ./modernizr.json -d ./public/modernizr.js -u",
    "build:patch": "sed -i -e 's/if(c\\.initial) return;/if(!c||c.initial) return;/g' ./node_modules/extract-text-webpack-plugin/index.js",
    "build:gatsby": "node --max-old-space-size=8192 ./node_modules/.bin/gatsby build",
    "clean": "rm -rf .cache && rm -rf public",
    "optimize": "npm-run-all --parallel optimize:*",
    "optimize:webp": "cd ./src && webpc -r",
    "optimize:svg": "find ./src -type f -name '*.svg' -print0 | xargs -0 -n 1 -P 6 svgo --enable=removeXMLNS --enable=removeTitle --enable=removeStyleElement --multipass",
    "optimize:images": "imagemin src/images/* --out-dir=src/images",
    "optimize:static": "imagemin static/* --out-dir=static",
    "qa": "npm-run-all --parallel qa:*",
    "qa:lint": "eslint --cache -f unix .",
    "qa:stylint": "stylint ./src",
    "test": "npm-run-all --parallel qa",
    "deploy": "npm-run-all build deploy:firebase",
    "deploy:firebase": "firebase deploy --non-interactive --token $FIREBASE_TOKEN"
  },
  "dependencies": {
    "@jacobmischka/gatsby-plugin-react-svg": "^1.0.0",
    "add": "^2.0.6",
    "autoprefixer": "^7.2.3",
    "axios": "^0.17.0",
    "camelcase-keys": "^4.1.0",
    "classnames": "^2.2.5",
    "gatsby": "^1.9.127",
    "gatsby-link": "^1.6.22",
    "gatsby-plugin-canonical-urls": "^1.0.8",
    "gatsby-plugin-feed": "^1.3.15",
    "gatsby-plugin-google-analytics": "^1.0.11",
    "gatsby-plugin-manifest": "^1.0.8",
    "gatsby-plugin-netlify": "^1.0.6",
    "gatsby-plugin-nprogress": "^1.0.10",
    "gatsby-plugin-offline": "^1.0.10",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "gatsby-plugin-react-next": "^1.0.7",
    "gatsby-plugin-sitemap": "^1.2.5",
    "gatsby-plugin-stylus": "^1.1.10",
    "linkify-urls": "^1.4.0",
    "modernizr": "^3.5.0",
    "moment": "^2.19.2",
    "multi-sort": "^1.0.1",
    "netlify-identity-widget": "^1.2.0",
    "number-to-words": "^1.2.3",
    "pascal-case": "^2.0.1",
    "poststylus": "^1.0.0",
    "query-string": "^5.0.1",
    "responsive-grid": "^1.1.0",
    "sentence-case": "^2.1.1",
    "stylus-mixins": "^0.4.0",
    "svgo": "^1.0.3",
    "uuid": "^3.1.0",
    "yarn": "^1.3.2"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "firebase-tools": "^3.17.4",
    "imagemin-cli": "^3.0.0",
    "npm-run-all": "^4.1.1",
    "stylint": "^1.5.9",
    "webp-converter-cli": "^0.0.5"
  }
}
