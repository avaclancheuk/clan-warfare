{
  "name": "destiny-clan-warfare",
  "description": "Destiny Clan Warfare",
  "version": "4.0.0",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "engines": {
    "node": "^10.14.1",
    "yarn": "^1.12.3"
  },
  "scripts": {
    "start": "npm-run-all --parallel start:*",
    "start:dev": "react-static start",
    "start:storybook": "start-storybook -p 9001 -c .storybook",
    "start:jest": "jest --watch",
    "build": "npm-run-all build:full",
    "build-skip": "if-env-defined INCOMING_HOOK_TITLE && npm-run-all build:incremental || npm-run-all build:full",
    "build:bundle": "react-static bundle",
    "build:full": "react-static build",
    "build:incremental": "react-static export --incremental",
    "build:storybook": "build-storybook -c .storybook -o ./docs",
    "build:percy": "percy-storybook --build_dir=./docs --widths=300,550,960",
    "clean": "rm -rf {artifacts,dist,docs,tmp}",
    "serve": "serve ./dist -p 9000",
    "optimize": "npm-run-all optimize:*",
    "optimize:svg": "find ./src -type f -name '*.svg' -print0 | xargs -0 -n 1 -P 6 svgo --enable=removeXMLNS --enable=removeTitle --enable=removeStyleElement --multipass",
    "optimize:images": "imagemin src/images/* --out-dir=src/images",
    "optimize:public": "imagemin public/* --out-dir=public",
    "qa": "npm-run-all --parallel qa:*",
    "qa:lint": "eslint --cache -f unix .",
    "qa:stylint": "stylint ./src",
    "test": "npm-run-all --parallel qa test:*",
    "test:jest": "jest && codecov",
    "test-skip:percy": "npm-run-all build:storybook build:percy",
    "deploy": "npm-run-all build deploy:firebase",
    "deploy:firebase": "firebase deploy --non-interactive --token $FIREBASE_TOKEN"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@reach/router": "^1.2.1",
    "@svgr/webpack": "^4.2.0",
    "autolinker": "^1.6.2",
    "autoprefixer": "^7.2.3",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "convert-hrtime": "^2.0.0",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "fs-extra": "^4.0.2",
    "listr": "^0.14.3",
    "moment": "^2.19.2",
    "number-to-words": "^1.2.3",
    "poststylus": "^1.0.0",
    "query-string": "^5.0.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-ga-donottrack": "^1.0.0",
    "react-static": "^7.0.6",
    "react-static-plugin-reach-router": "^7.0.0",
    "react-static-plugin-sitemap": "^7.0.0",
    "react-static-plugin-source-filesystem": "^7.0.0",
    "react-tag-autocomplete": "^5.8.0",
    "react-virtualized": "^9.20.1",
    "responsive-grid": "^1.1.0",
    "rss": "^1.2.2",
    "sentence-case": "^2.1.1",
    "style-loader": "^0.20.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "stylus-mixins": "^0.4.0",
    "svgo": "^1.0.3",
    "thenby": "^1.2.6",
    "webpack-pwa-manifest": "^4.0.0",
    "yarn": "^1.12.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@percy-io/in-percy": "^0.1.11",
    "@percy-io/percy-storybook": "^2.1.0",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "babel-loader": "^8.0.5",
    "codecov": "^3.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "firebase-tools": "^3.17.4",
    "identity-obj-proxy": "^3.0.0",
    "if-env-defined": "^1.0.0",
    "imagemin-cli": "^3.0.0",
    "jest": "^24.1.0",
    "jest-svg-transformer": "^1.0.0",
    "mockdate": "^2.0.2",
    "npm-run-all": "^4.1.1",
    "serve": "^7.1.3",
    "stylint": "^1.5.9"
  }
}
