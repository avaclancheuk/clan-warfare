{
  "name": "@newhighsco/destiny-clan-warfare",
  "description": "Destiny Clan Warfare",
  "version": "6.0.0",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "private": true,
  "config": {
    "title": "Destiny Clan Warfare",
    "logo": "logo.svg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/destiny-clan-warfare.git"
  },
  "homepage": "https://destinyclanwarfare.com/",
  "bugs": {
    "url": "https://github.com/newhighsco/destiny-clan-warfare/issues"
  },
  "files": [],
  "scripts": {
    "clean": "rm -rf {.next}",
    "start": "next dev",
    "start:storybook": "start-storybook --static-dir public --docs --no-manager-cache",
    "build": "next build",
    "prebuild:storybook": "rm -rf storybook",
    "build:storybook": "build-storybook --output-dir storybook --static-dir public --docs --quiet",
    "test": "yarn lint",
    "snapshot": "chromatic --storybook-build-dir storybook --auto-accept-changes main --exit-once-uploaded",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.json,.jsx,.mdx .",
    "lint:css": "stylelint '**/*.{css,scss}' --cache",
    "format": "npm-run-all --parallel format:*",
    "format:js": "yarn lint:js --fix",
    "format:css": "yarn lint:css --fix"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "4.0.3",
    "@newhighsco/chipset": "6.8.1",
    "@newhighsco/next-plugin-svgr": "3.0.27",
    "next": "11.1.2",
    "next-compose-plugins": "2.2.1",
    "next-transpile-modules": "8.0.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "6.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "1.42.1"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@commitlint/cli": "13.2.0",
    "@newhighsco/browserslist-config": "1.0.3",
    "@newhighsco/commitlint-config": "1.0.15",
    "@newhighsco/editor-config": "1.1.2",
    "@newhighsco/eslint-config": "2.3.29",
    "@newhighsco/postcss-config": "3.1.25",
    "@newhighsco/prettier-config": "2.0.14",
    "@newhighsco/release-config": "1.0.71",
    "@newhighsco/storybook-preset": "4.3.12",
    "@newhighsco/stylelint-config": "2.0.46",
    "@storybook/react": "6.3.8",
    "babel-loader": "8.2.2",
    "chromatic": "5.10.1",
    "eslint": "7.32.0",
    "husky": "7.0.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "semantic-release": "18.0.0",
    "stylelint": "13.13.1"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config/react"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "next"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
