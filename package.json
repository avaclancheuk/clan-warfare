{
  "name": "@newhighsco/destiny-clan-warfare",
  "description": "Destiny Clan Warfare",
  "version": "6.0.0",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/destiny-clan-warfare.git"
  },
  "homepage": "https://destinyclanwarfare.com/",
  "bugs": {
    "url": "https://github.com/newhighsco/destiny-clan-warfare/issues"
  },
  "files": [],
  "scripts": {
    "prepare": "is-ci || husky install",
    "clean": "rm -rf {.next,coverage,public/{robots.txt,sitemap*.xml},storybook}",
    "start": "netlify dev",
    "start:next": "next dev --port 9001",
    "start:storybook": "start-storybook --port 9002 --docs --no-manager-cache",
    "build": "next build",
    "postbuild": "next-sitemap",
    "build:storybook": "build-storybook --output-dir storybook --docs --quiet",
    "serve": "next start",
    "pretest": "yarn lint",
    "test": "jest",
    "snapshot": "chromatic --storybook-build-dir storybook --auto-accept-changes main --exit-once-uploaded",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "yarn lint-staged:js --cache .",
    "lint:css": "yarn lint-staged:css --cache '**/*.?(s)css'",
    "lint-staged:js": "eslint --ignore-path .gitignore",
    "lint-staged:css": "stylelint --ignore-path .gitignore",
    "format": "npm-run-all --parallel format:*",
    "format:js": "yarn lint:js --fix",
    "format:css": "yarn lint:css --fix",
    "pregenerate:ssl": "mkdir -p ssl",
    "generate:ssl": "openssl req -x509 -out ssl/localhost.crt -keyout ssl/localhost.key -newkey rsa:2048 -nodes -sha256 -subj '/CN=localhost' -extensions EXT -config certconf"
  },
  "dependencies": {
    "@newhighsco/chipset": "6.14.11",
    "@newhighsco/next-plugin-svgr": "3.0.79",
    "@newhighsco/press-start": "2.2.10",
    "autolinker": "4.0.0",
    "bungie-api-ts": "4.21.0",
    "dayjs": "1.11.7",
    "json-bigint": "1.0.0",
    "next": "13.1.5",
    "next-auth": "3.29.10",
    "next-compose-plugins": "2.2.1",
    "next-sitemap": "3.1.47",
    "param-case": "3.0.4",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-virtualized-auto-sizer": "1.0.7",
    "react-window": "1.8.8",
    "sass": "1.57.1",
    "sentence-case": "3.0.4"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@commitlint/cli": "17.4.2",
    "@netlify/plugin-nextjs": "4.30.2",
    "@newhighsco/browserslist-config": "1.1.0",
    "@newhighsco/commitlint-config": "1.1.13",
    "@newhighsco/editor-config": "1.2.0",
    "@newhighsco/eslint-config": "4.0.3",
    "@newhighsco/postcss-config": "3.5.109",
    "@newhighsco/prettier-config": "2.1.7",
    "@newhighsco/release-config": "1.3.15",
    "@newhighsco/storybook-preset": "4.5.60",
    "@newhighsco/stylelint-config": "3.1.32",
    "@newhighsco/svgo-config": "1.0.0",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/react": "6.5.16",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.4.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-window": "1.8.5",
    "alias-hq": "6.1.0",
    "babel-loader": "9.1.2",
    "chromatic": "6.14.0",
    "eslint": "8.32.0",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-testing-library": "5.10.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.4.0",
    "jest-environment-jsdom": "29.4.0",
    "lint-staged": "13.1.0",
    "netlify-cli": "12.9.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.3",
    "semantic-release": "20.1.0",
    "stylelint": "14.16.1",
    "typescript": "4.9.4",
    "webpack": "5.75.0"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config"
    ],
    "overrides": [
      {
        "files": [
          "**/*.spec.ts?(x)"
        ],
        "plugins": [
          "testing-library",
          "jest-dom"
        ],
        "rules": {
          "testing-library/no-container": "off",
          "testing-library/no-node-access": "off"
        },
        "extends": [
          "plugin:testing-library/react",
          "plugin:jest-dom/recommended"
        ]
      }
    ]
  },
  "lint-staged": {
    "*.{[j|t]s?(on|x),mdx}": "yarn lint-staged:js --fix",
    "*.?(s)css": "yarn lint-staged:css --fix"
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
