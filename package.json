  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/destiny-clan-warfare.git"
  },
  "homepage": "https://destinyclanwarfare.com/",
  "bugs": {
    "url": "https://github.com/newhighsco/destiny-clan-warfare/issues"
  },
  "files": [],
  "scripts": {
    "clean": "rm -rf {.next}",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "yarn lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.json,.jsx,.mdx .",
    "lint:css": "stylelint '**/*.{css,scss}' --cache",
    "format": "npm-run-all --parallel format:*",
    "format:js": "yarn lint:js --fix",
    "format:css": "yarn lint:css --fix",
    "storybook": "start-storybook --docs"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "4.0.3",
    "@newhighsco/chipset": "6.7.36",
    "@newhighsco/next-plugin-svgr": "3.0.27",
    "next": "11.1.2",
    "next-compose-plugins": "2.2.1",
    "next-transpile-modules": "8.0.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "6.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "1.38.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@commitlint/cli": "13.1.0",
    "@newhighsco/browserslist-config": "1.0.3",
    "@newhighsco/commitlint-config": "1.0.14",
    "@newhighsco/editor-config": "1.1.2",
    "@newhighsco/eslint-config": "2.3.18",
    "@newhighsco/postcss-config": "3.1.20",
    "@newhighsco/prettier-config": "2.0.12",
    "@newhighsco/release-config": "1.0.66",
    "@newhighsco/storybook-preset": "4.1.2",
    "@newhighsco/stylelint-config": "2.0.43",
    "@storybook/react": "6.3.7",
    "babel-loader": "8.2.2",
    "eslint": "7.32.0",
    "husky": "7.0.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "semantic-release": "17.4.7",
    "stylelint": "13.13.1"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config/react"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
