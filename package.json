{
  "name": "clan-warfare",
  "version": "0.1.0",
  "author": "Avalanche UK <admins@avaclanche.uk>",
  "license": "ISC",
  "engines": {
    "node": "^8.9.0",
    "yarn": "^0.17.9"
  },
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "optimize": "npm-run-all --parallel optimize:*",
    "optimize:svg": "find ./src -type f -name '*.svg' -print0 | xargs -0 -n 1 -P 6 svgo --enable=removeXMLNS --enable=removeTitle --enable=removeStyleElement --multipass --pretty --indent=2",
    "qa": "npm-run-all --parallel qa:*",
    "qa:lint": "eslint --cache -f unix .",
    "qa:stylint": "stylint ./src",
    "test": "npm-run-all --parallel qa"
  },
  "dependencies": {
    "@jacobmischka/gatsby-plugin-react-svg": "^1.0.0",
    "axios": "^0.17.0",
    "classnames": "^2.2.5",
    "gatsby": "^1.9.73",
    "gatsby-link": "^1.6.22",
    "gatsby-plugin-google-analytics": "^1.0.11",
    "gatsby-plugin-manifest": "^1.0.8",
    "gatsby-plugin-offline": "^1.0.10",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "gatsby-plugin-stylus": "^1.1.10",
    "responsive-grid": "^1.1.0",
    "stylus-mixins": "^0.4.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "npm-run-all": "^4.1.1",
    "stylint": "^1.5.9",
    "svgo": "^1.0.1"
  }
}
